#include <stdio.h>

int main()
{
    int n;
    scanf("%d", &n);
    int count500=0, count100=0, count50=0, count10=0, count5=0, count1=0;
    while(n>=500)
    {
        n-=500;
        count500++;
    }
    while(n>=100)
    {
        n-=100;
        count100++;
    }
    while (n>=50)
    {
        n-=50;
        count50++;
    }
    while (n>=10)
    {
        n-=10;
        count10++;
    }
    while (n>=5)
    {
        n-=5;
        count5++;
    }
    while (n>=1)
    {
        n-=1;
        count1++;
    }
    printf("%d note(s) of 500\n%d note(s) of 100\n%d note(s) of 50\n%d note(s) of 10\n%d note(s) of 5\n%d note(s) of 1", count500,count100,count50,count10,count5,count1);
    return 0;
}
